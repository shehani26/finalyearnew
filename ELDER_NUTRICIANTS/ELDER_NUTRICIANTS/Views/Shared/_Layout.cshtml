    <script>
        function confirmLogout(logoutUrl) {
            console.log("Logout function called with URL:", logoutUrl);

            Swal.fire({
                title: '<span class="text-gradient">Are you sure?</span>',
                html: '<p class="text-muted">You will be logged out of your account.</p>',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#e91e63', // Material design primary color
                cancelButtonColor: '#757575', // Material design gray
                confirmButtonText: '<i class="material-symbols-rounded me-2">logout</i> Yes, logout',
                cancelButtonText: '<i class="material-symbols-rounded me-2">close</i> Cancel',
                background: '#fff',
                backdrop: `rgba(0,0,0,0.4)`,
                showClass: {
                    popup: 'animate__animated animate__fadeInDown animate__faster'
                },
                hideClass: {
                    popup: 'animate__animated animate__fadeOutUp animate__faster'
                },
                customClass: {
                    title: 'swal-title',
                    confirmButton: 'btn btn-danger',
                    cancelButton: 'btn btn-outline-secondary',
                    popup: 'rounded-lg shadow-lg'
                },
                buttonsStyling: false,
                focusConfirm: false,
                timerProgressBar: true,
                padding: '2em'
            }).then((result) => {
                if (result.isConfirmed) {
                    console.log("Confirmed logout, redirecting to:", logoutUrl);

                    // Show loading state
                    Swal.fire({
                        title: 'Logging out...',
                        html: 'Please wait while we sign you out securely',
                        allowOutsideClick: false,
                        showConfirmButton: false,
                        willOpen: () => {
                            Swal.showLoading();
                        }
                    });

                    // Short delay to show the loading animation
                    setTimeout(() => {
                        window.location.href = logoutUrl;
                    }, 800);
                }
            });
        }
    </script>

